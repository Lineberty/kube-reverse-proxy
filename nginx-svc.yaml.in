apiVersion: v1
kind: Service
metadata:
  name: kube-reverse-proxy
  labels:
    k8s-app: kube-reverse-proxy
    kubernetes.io/cluster-service: "true"
    kubernetes.io/name: "KubeReverseProxy"
spec:
  selector:
    k8s-app: kube-reverse-proxy
  ports:
  - name: http
    port: 80
    protocol: tcp
    targetPort: 80
    nodePort: 30080
  - name: https
    port: 443
    protocol: tcp
    targetPort: 443
    nodePort: 30443
  type: NodePort
